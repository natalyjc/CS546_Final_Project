<div class="admin-dashboard">
  <div class="admin-header">
    <h2>Admin Dashboard</h2>
    <p>Welcome, Admin!</p>
    <form action="/logout" method="GET">
      <button type="submit" class="logout-btn">Logout</button>
    </form>
  </div>

  <table class="user-table">
    <thead>
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Is Admin?</th>
      </tr>
    </thead>
    <tbody>
      {{#each users}}
      <tr>
        <td>{{this.firstName}}</td>
        <td>{{this.lastName}}</td>
        <td>{{this.email}}</td>
        <td>{{this.isAdmin}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>
